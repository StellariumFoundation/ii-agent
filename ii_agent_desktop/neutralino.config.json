{
  "applicationId": "io.iiagent.desktop",
  "version": "0.1.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/app/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.exit",
    "os.showNotification",
    "os.showSaveDialog",
    "os.showOpenDialog",
    "filesystem.readFile",
    "filesystem.readBinaryFile",
    "filesystem.getPath",
    "storage.setData",
    "storage.getData",
    "debug.log",
    "extensions.spawnProcess",
    "extensions.updateProcessInput",
    "extensions.killProcess",
    "events.*"
  ],
  "modes": {
    "window": {
      "title": "II-Agent Desktop",
      "width": 800,
      "height": 600,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/app/assets/icon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "maximizable": true
    }
  },
  "cli": {
    "binaryName": "ii-agent-desktop",
    "resourcesPath": "/app/",
    "clientLibrary": "/app/js/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0"
  }
}
